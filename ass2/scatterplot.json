{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Lyra-Z/-3139-homework_w9/main/data/restaurantRank.csv"
    },
    "title":"Tasting Menu Price (AUD) for Each Restaurant",
    "width": 700,
    "height": 500,
